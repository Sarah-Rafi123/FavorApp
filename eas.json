{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "testing": {
      "android": {
        "buildType": "apk",
        "distribution": "internal",
        "env": {
          "EXPO_PUBLIC_ENVIRONMENT": "testing"
        }
      },
      "ios": {
        "distribution": "internal",
        "env": {
          "EXPO_PUBLIC_ENVIRONMENT": "testing"
        }
      }
    },
    "production": {
      "autoIncrement": false
    }
  },
  "submit": {
    "production": {}
  }
}
