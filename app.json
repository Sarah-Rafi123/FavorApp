      }
    },
    "android": {
      "icon": "./assets/logo_app_icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_app_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.favor.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDt1zyVSt1snXRBteLuH9ngKmE8ABve268"
